    "react-google-charts": "^1.5.5",
    "react-router": "^3.0.2",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-react-session": "^1.0.8"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
